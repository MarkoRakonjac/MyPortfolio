{% include "locator_icons" %}<div class=" js-store-locator hidden" data-geoip="{{ section.settings.geoip }}" >    <div class="locator">        {% if section.settings.locator_bg!=blank %}            <div class="rimage-outer-wrapper lazyload fade-in locator__bg"                 data-bgset="{% include 'bgset', image: section.settings.locator_bg %}"                 data-sizes="auto"                 data-parent-fit="cover">                <noscript>                    <div class="rimage-wrapper"                         style="padding-top:{{ 1 | divided_by: section.settings.cover_image.aspect_ratio | times: 100 }}%">                        <img src="{% comment ls-rpl %}{{ section.settings.locator_bg | img_url: '1024x1024' }}{% endcomment %}{%- capture LS_IMG_URL -%}{%- render "ls-img-url", image: section.settings.locator_bg, img_url: '1024x1024' -%}{%- endcapture -%}{{ LS_IMG_URL }}{% comment /ls-rpl %}{% endcomment %}"                             alt="{% comment ls-rpl %}{{ section.settings.locator_bg.alt | escape }}{% endcomment %}{%- capture LS_ALT -%}{%- render "ls-alt", image: section.settings.locator_bg -%}{%- endcapture -%}{{ LS_ALT | escape }}{% comment /ls-rpl %}{% endcomment %}" class="rimage__image">                    </div>                </noscript>            </div>        {% endif %}        <div class="locator__holder">            <div class="locator__top">                {% if section.settings.locator_ttl!=blank %}                    <div class="locator__ttl">{{ section.settings.locator_ttl }}</div>                {% endif %}                <div class="locator__item">                    <div class="locator__item-img locator__item-img--lg">                        <svg class="svg-icon locator__icon locator__icon--lg">                            <use xlink:href="" class="js-store-locator-icon"></use>                        </svg>                    </div>                    <div class="locator__item-ttl locator__item-ttl--lg js-store-locator-ttl"></div>                </div>            </div>            <div class="locator__bottom">                {% include 'svg-icon', icon_name: 'line' , icon_class: 'svg-icon--line locator__line' %}                {% if section.settings.locator_subttl!=blank %}                    <div class="locator__subttl">{{ section.settings.locator_subttl }}</div>                {% endif %}                {% if section.blocks.size > 0 %}                    <div class="locator__list">                        {% for block in section.blocks %}                            {% if block.settings.url != blank %}                                <a href="{{ block.settings.url }}" class="locator__item locator__list-item ">                                    <div class="locator__item-img">                                        {% assign class = "svg-icon--" | append: "block.setting.lang_type" | append: " locator__icon" %}                                        {% include 'svg-icon', icon_name: block.settings.lang_type , icon_class: class %}                                    </div>                                    {% if block.settings.title!=blank %}                                        <div class="locator__item-ttl">                                            {{ block.settings.title }}                                        </div>                                    {% endif %}                                </a>                            {% endif %}                        {% endfor %}                    </div>                {% endif %}            </div>        </div>    </div></div>{% schema %}    {    "name": "Store locator",    "settings": [    {    "type": "text",    "id": "geoip",    "label": "GeoIP"    }    ],    "blocks": [    {    "type": "locator",    "name": "Location",    "settings": [    {    "type": "text",    "id": "title",    "label": "Title"    },    {    "type": "url",    "id": "url",    "label": "Link"    },    {    "type": "select",    "id": "lang_type",    "options": [    { "value": "es", "label": "Spain"},    { "value": "eu", "label": "Europe"},    { "value": "gb", "label": "United Kingdom"},    { "value": "world", "label": "Rest of the world"},    { "value": "am", "label": "Latin America"}    ],    "label": "Type",    "default": "es"    }    ]    }    ]    }{% endschema %}